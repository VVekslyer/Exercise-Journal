#: import FloatingPlusButton widgets.FloatingPlusButton

<WorkoutItem@MDSwiperItem>:
    RelativeLayout:
        FitImage:
            source: "./assets/images/deadlift.png"
            radius: [20,]

        MDLabel:
            text: "Hello?"
            font_style: "H5"
            size_hint_y: None
            height: self.texture_size[1]
            pos_hint: {"top": 1,"center_x" : .5}
            color: (1, 1, 1, 1)

<Home>:
    BoxLayout:
        orientation: 'vertical'
        MDLabel:
            text: 'Home'
            text_size: self.size
            valign: 'top'
            halign: 'left'
            padding_x: '24dp'
            padding_y: '12dp'
            text_color: (0,0,0,1)
            font_style: 'H4'
            font_name: 'assets/fonts/Inter-SemiBold.ttf'
        MDSwiper:
            size_hint_y: None
            height: root.height - dp(40)
            y: root.height - self.height - dp(20)
            WorkoutItem:
            WorkoutItem:
            WorkoutItem:
            

        FloatingPlusButton: